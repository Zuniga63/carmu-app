{
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "tabWidth": 2,
  "printWidth": 120,
  "endOfLine": "lf",
  "semi": true,
  "plugins": ["prettier-plugin-tailwindcss"]
}
